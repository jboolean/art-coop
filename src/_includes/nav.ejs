<nav class="main-nav">
  <ul>
    <% collections[collectionName].filter((section)=>
    !section.data.hideInNavigation).forEach((section)=> { %>
    <li>
      <a href="#<%= section.data.anchor %>"> <%= section.data.title %> </a>
    </li>
    <% }) %> <% (extraNavLinks || []).forEach(({href, isExternal, text,
    isCta})=> { %>
    <li class="<% if (isCta) {%>cta <%}%>">
      <a href="<%= href %>" <% if (isExternal) { %>
        target="_blank" rel="noopener noreferrer" <% } %>> <%= text %>
      </a>
    </li>
    <% }) %>
  </ul>
</nav>
